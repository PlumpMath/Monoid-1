<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MonoidAlpha"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="MonoidAlpha.App"
             StartupUri="MainWindow.xaml">
	<Application.Resources>

		<System:String x:Key="START_GAME">开始游戏</System:String>
		
		<SolidColorBrush x:Key="BrushText">
			<SolidColorBrush.Color>
				#FF000000
			</SolidColorBrush.Color>
		</SolidColorBrush>
		
		<SolidColorBrush x:Key="BrushTextActive">
			<SolidColorBrush.Color>
				#FF3399FF
			</SolidColorBrush.Color>
		</SolidColorBrush>
		
		<SolidColorBrush x:Key="BackgroundLight" Color="White"/>
		<System:String x:Key="LOAD_GAME">载入游戏</System:String>

	</Application.Resources>
</Application>
